{
  "name": "claude-magic-ui",
  "version": "1.0.0",
  "description": "A natural language UI component generator for Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,json}",
